generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    // url      = env("DATABASE_URL")
    url      = "file:./dev.db.sqlite"
}

model Recipe {
    id        String   @id @default(cuid())
    title     String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
